.sample-detail.scroll-y
	.row.form-horizontal
		.col-xs-8
			input.form-control(type='text', ng-model='s.queryString', placeholder='Query String', disabled)
		.col-xs-4
			button.btn.btn-info.btn-block(ng-click='clickQuery()', ng-disabled="!s.isQueryable") Query
	.row
		.col-xs-12.form-horizontal
			.form-group(ng-repeat='p in s.params | filter: {visible: true}')
				label.col-xs-4.control-label{{p.name}}
				.col-xs-8
					input.form-control(ng-model='p.defaultValue', ng-disabled="!p.editable")

		.col-xs-4.col-xs-offset-8
	.row
		.col-xs-12.bg-dark
			json-formatter(open='1', json='s.result').json-formatter-dark